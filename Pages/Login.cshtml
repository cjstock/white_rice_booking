@page
@model LoginModel
@addTagHelper *, Microsoft.AspNetCore.Mcv.TagHelpers
@{
    ViewData["Title"] = "Login"; // Adds "My Trips" to ViewData dictionary
}
<h1>@ViewData["Title"]</h1>

<form method="post">
    <div>
    Email:
    <input asp-for="@Model.UserAccount.Email" />
    </div>
    <div>
    Password:
    <input asp-for="@Model.UserAccount.Password" />
    </div>

    <input type="submit"/>
</form>