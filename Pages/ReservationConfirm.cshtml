@page 
@model ReservationConfirmModel
@{
    ViewData["Title"] = "Order Confirmation"; // Adds "My Trips" to ViewData dictionary
}
<div>
    <p>Depart ID: @Model.depart_ID</p>
    <p>Return ID: @Model.return_ID</p>
    <p>First Name: @Model.FirstName</p>
    <p>Last Name: @Model.LastName</p>
    <p>Street Address: @Model.StreetName</p>
    <p>City: @Model.City</p>
    <p>State: @Model.State</p>
    <p>Zip Code: @Model.ZipCode</p>
    <p>Route ID: @Model.route_id</p>
    <p>Departure Flight ID: @Model.depart_flight_id</p>
    <p>Arrival Flight ID: @Model.arrival_id</p>
    <p>Departing Airport: @Model.depart_airport_name</p>
    <p>Departing City: @Model.depart_city_name</p>
    <p>Arriving Airport: @Model.arrival_airport_name</p>
    <p>Arriving City: @Model.arrival_city_name</p>
</div>